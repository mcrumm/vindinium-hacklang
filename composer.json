{
  "name": "mcrumm/vindinium-hacklang",
  "description": "Vindinium in (mostly) hack-strict",
  "license": "MIT",
  "keywords": [ "vindinium", "hack", "hacklang", "hhvm" ],
  "require": {
    "guzzlehttp/guzzle-services": "0.*",
    "symfony/console": "2.5.*",
    "symfony/yaml": "2.5.*"
  },
  "require-dev": {
      "hackpack/hackunit": "0.*"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
        "Hackdinium\\": "src/"
    },
    "files": [ "src/models.hh" ]
  },
  "bin": [ "bin/hackdinium" ]
}
