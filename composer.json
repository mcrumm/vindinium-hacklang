{
  "name": "mcrumm/vindinium-hacklang",
  "description": "Vindinium in (mostly) hack-strict",
  "license": "MIT",
  "keywords": [ "vindinium", "hack", "hacklang", "hhvm" ],
  "require": {
    "hhvm": ">=3.3.0",
    "mcrumm/vindinium-guzzle": "~0.2.1",
    "aura/cli": "2.0.*@dev"
  },
  "require-dev": {
      "hackpack/hackunit": "~0.3.3"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
        "Hackdinium\\": "src/"
    },
    "files": [ "src/functions.hh", "src/models.hh" ]
  },
  "config": {
      "bin-dir": "bin"
  },
  "bin": [ "bin/hackdinium" ]
}
